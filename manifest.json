{
	"name": "Wingbeat",
	"description": "Wingbeat",
	"version": "0.2.4",
	"manifest_version": 3,
	"icons": {
		"16": "/assets/16.png",
		"32": "/assets/32.png",
		"48": "/assets/48.png",
		"128": "/assets/128.png",
		"144": "/assets/144.png",
		"192": "/assets/192.png",
		"512": "/assets/512.png"
	},
	"author": "malisipi",
	"action": {
		"default_popup": "index.html",
		"default_title": "Wingbeat"
	},
	"background": {
		"service_worker": "assets/welcome.js",
		"type": "module"
	},
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"https://*.twitter.com/*",
				"https://*.x.com/*"
			],
			"js": ["wingbeat.js"]
		}
	]
}
